{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "plugin:vue/vue3-essential",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "2021",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "quotes": "off",
        "no-debugger": "off",
        "semi":"off",
        "comma-dangle":"off",
        "indent":"off"
    }
}
